  },
  "homepage": "https://github.com/DaryaPukhliak/Deliver-X#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "node-sass": "^7.0.1",
    "postcss-cli": "^9.1.0"
  }
}
