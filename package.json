{
  "name": "deliver-x",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sass": "sass scss/styles.scss css/styles.css",
    "scss:autoprefixer": "postcss -u autoprefixer --autoprefixer.browsers \"last 2 versions\" -r css/styles.css",
    "watch": "sass --watch scss/styles.scss:css/styles.css",
    "build": "npm run sass && npm run scss:autoprefixer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaryaPukhliak/Deliver-X.git"
  },
